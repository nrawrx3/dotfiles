{
  "[cpp]": {
    "editor.defaultFormatter": "xaver.clang-format",
    "editor.tabSize": 2
  },
  "[css]": {
    "editor.defaultFormatter": "vscode.css-language-features"
  },
  "[dart]": {
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.selectionHighlight": false,
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "onlySnippets",
    "editor.wordBasedSuggestions": "off"
  },
  "[dockercompose]": {
    "editor.autoIndent": "advanced",
    "editor.defaultFormatter": "redhat.vscode-yaml",
    "editor.insertSpaces": true,
    "editor.tabSize": 2
  },
  "[github-actions-workflow]": {
    "editor.defaultFormatter": "redhat.vscode-yaml",
    "editor.insertSpaces": true,
    "editor.tabSize": 2
  },
  "[glsl]": {
    "editor.defaultFormatter": "xaver.clang-format"
  },
  "[go]": {
    "editor.codeActionsOnSave": {
      "source.organizeImports": "explicit"
    },
    "editor.formatOnSave": true,
    "editor.insertSpaces": false,
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "editor.tabSize": 2
  },
  "[graphql]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 2
  },
  "[javascript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features",
    "editor.tabSize": 2,
    "editor.useTabStops": false
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features",
    "editor.tabSize": 2,
    "editor.useTabStops": false
  },
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features",
    "editor.tabSize": 2,
    "editor.useTabStops": true
  },
  "[jupyter]": {
    "editor.tabSize": 4
  },
  "[markdown]": {
    "editor.defaultFormatter": "yzhang.markdown-all-in-one",
    "editor.tabSize": 4,
    "editor.useTabStops": false
  },
  "[proto3]": {
    "editor.defaultFormatter": "xaver.clang-format",
    "editor.tabSize": 2,
    "editor.useTabStops": false
  },
  "[python]": {
    "editor.defaultFormatter": "ms-python.black-formatter",
    "editor.tabSize": 4
  },
  "[rust]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 4
  },
  "[sql]": {
    "editor.defaultFormatter": "adpyke.vscode-sql-formatter",
    "editor.insertSpaces": true,
    "editor.tabSize": 2
  },
  "[typescript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features",
    "editor.formatOnSave": true,
    "editor.insertSpaces": true,
    "editor.tabSize": 2
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "vscode.typescript-language-features",
    "editor.formatOnSave": true,
    "editor.insertSpaces": true,
    "editor.tabSize": 2
  },
  "[wgsl]": {
    "editor.defaultFormatter": "xaver.clang-format",
    "editor.insertSpaces": true,
    "editor.tabSize": 2
  },
  "[yaml]": {
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "[zig]": {
    "editor.defaultFormatter": "ziglang.vscode-zig",
    "editor.formatOnSave": true,
    "editor.tabSize": 4,
    "files.eol": "\n"
  },
  "cmake.options.statusBarVisibility": "visible",
  "cmake.pinnedCommands": [
    "workbench.action.tasks.configureTaskRunner",
    "workbench.action.tasks.runTask"
  ],
  "cmake.preferredGenerators": [
    "Unix Makefiles",
    "Ninja"
  ],
  "cursor.composer.textSizeScale": 1.3,
  "cursor.cpp.disabledLanguages": [
    "markdown",
    "plaintext"
  ],
  "dart.openDevTools": "flutter",
  "debug.console.fontFamily": "SF Mono",
  "debug.console.fontSize": 15,
  "debug.console.wordWrap": false,
  "dimloglines.opacity": 0.4,
  "dimloglines.regExps": [
    "\\w*LOG\\s*\\([\\s\\S]*?\\)?\\s*?;",
    "\\w*[lL][oO][gG].*?\\.(Write|assert|debug|report|info|information|success|warning|error|fail|exception|critical)\\s*\\([\\s\\S]*?\\)?\\s*?;",
    "\\w*[lL]og(ger)?.*?\\.(Debug|Report|Info|Success|Warning|Error|Fail|Exception|Critical|Fatal|Fatalln|Panic|Panicln|Print|Println)\\w*\\s*\\([\\s\\S]*?\\)[\\t ]*$"
  ],
  "editor.accessibilitySupport": "off",
  "editor.bracketPairColorization.enabled": false,
  "editor.detectIndentation": false,
  "editor.fontFamily": "SF Mono",
  "editor.fontLigatures": true,
  "editor.fontSize": 15,
  "editor.fontWeight": "400",
  "editor.formatOnPaste": true,
  "editor.glyphMargin": true,
  "editor.guides.bracketPairsHorizontal": false,
  "editor.guides.highlightActiveBracketPair": false,
  "editor.guides.indentation": false,
  "editor.hideCursorInOverviewRuler": true,
  "editor.inlineSuggest.enabled": true,
  "editor.lineHeight": 0,
  "editor.lineNumbers": "on",
  "editor.minimap.enabled": true,
  "editor.minimap.maxColumn": 30,
  "editor.minimap.showSlider": "always",
  "editor.minimap.side": "left",
  "editor.overviewRulerBorder": false,
  "editor.renderLineHighlight": "gutter",
  "editor.renderWhitespace": "trailing",
  "editor.scrollbar.horizontal": "hidden",
  "editor.scrollbar.vertical": "hidden",
  "editor.scrollbar.verticalScrollbarSize": 5,
  "editor.showFoldingControls": "never",
  "editor.snippetSuggestions": "top",
  "editor.tabSize": 8,
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": [
          "comment",
          "comment.block",
          "comment.line",
          "comment.block.documentation",
          "entity.name.type.class",
          "entity.name.type",
          "entity.name.type.interface",
          "entity.name.type.enum",
          "entity.name.function"
        ],
        "settings": {
          "fontStyle": ""
        }
      }
    ]
  },
  "editor.trimAutoWhitespace": true,
  "editor.wordWrap": "on",
