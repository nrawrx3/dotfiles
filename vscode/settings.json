                                {
                                        "workbench.colorTheme": "Navy And Ivory",
                                },
                                {
                                        "workbench.colorTheme": "poimandres-noitalics",
                                },
                                {
                                        "workbench.colorTheme": "Keen (rainglow)",
                                },
                        ]
