
[options]
scroll_on_output = false
scroll_on_keystroke = true
audible_bell = false
mouse_autohide = false
allow_bold = true
dynamic_title = true
urgent_on_bell = true
clickable_url = true
font = Hack 8
scrollback_lines = 10000
search_wrap = true
#icon_name = terminal
geometry = 720x480

# "system", "on" or "off"
cursor_blink = off

# "block", "underline" or "ibeam"
cursor_shape = block

# $BROWSER is used by default if set, with xdg-open as a fallback
browser = firefox

# set size hints for the window
#size_hints = false

# emit escape sequences for other keys modified by Control
#modify_other_keys = false

[colors]

foreground = #bbbbbb
background = #112233
color0 = #445566
color8 = #445566
color1 = #dd3366
color9 = #dd3366
color2 = #99aa00
color10 = #99aa00
color3 = #bb9900
color11 = #bb9900
color4 = #3399dd
color12 = #3399dd
color5 = #6666dd
color13 = #6666dd
color6 = #339999
color14 = #339999
color7 = #bbbbbb
color15 = #bbbbbb
