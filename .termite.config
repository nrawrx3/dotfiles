[options]
scroll_on_output = false
scroll_on_keystroke = true
audible_bell = false
mouse_autohide = false
allow_bold = true
dynamic_title = true
urgent_on_bell = true
clickable_url = true
font = DejaVuSansMono 10
scrollback_lines = 1000
search_wrap = true
#icon_name = terminal
#geometry = 640x480

# "system", "on" or "off"
cursor_blink = off

# "block", "underline" or "ibeam"
cursor_shape = ibeam

# $BROWSER is used by default
#browser = firefox

# set size hints for the window
#size_hints = false

# emit escape sequences for other keys modified by Control
#modify_other_keys = false

[colors]
foreground=#ECE8CF
background=#2D2D2D
color0=#393939
color8=#474747
color1=#EE9379
color9=#FF8D80
color2=#E2FFC7
color10=#DBFFB3
color3=#FFFFB9
color11=#FFBE64
color4=#EDE7B4
color12=#E15848
color5=#E2BEB5
color13=#94C7B5
color6=#6EB49D
color14=#A2C3CC
color7=#dddddd
color15=#dddddd
[hints]
#font = DejaVuSansMono 10
#foreground = #dcdccc
#background = #3f3f3f
#active_foreground = #e68080
#active_background = #3f3f3f
#padding = 2
#border = #3f3f3f
#border_width = 0.5
#roundness = 2.0

# vim: ft=dosini cms=#%s
