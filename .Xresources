URxvt*geometry:					120x30
URxvt*loginShell:				true
URxvt*scrollBar:				false
URxvt*scrollWithBuffer:				true
!URxvt*fading:					30
URxvt*urgentOnBell:				true
!URxvt*cursorUnderline:				true

*internalBorder:				10

URxvt*perl-ext-common:				default,matcher
URxvt*matcher.button:				1
URxvt*url-launcher:				/usr/bin/firefox

URxvt.font:					xft:Consolas:size=10
URxvt.boldFont:					xft:Consolas:bold:size=10

URxvt*depth: 32

!-----------------------------------------
! XFT
!-----------------------------------------
Xft.dpi: 96
Xft.antialias: 1
Xft.hinting: 1
Xft.hintstyle: hintfull
Xft.rgba: rgb

*.foreground:   #fbf0d2
*.background:   #181512
*.cursorColor:  #839496

! black
*.color0:       #282828
*.color8:       #454545

! red
*.color1:       #e93c93
*.color9:       #a87181

! green
*.color2:       #00a17c
*.color10:      #76a571

! yellow
*.color3:       #f79031
*.color11:      #d0a845

! blue
*.color4:       #00c0ce
*.color12:      #008ba5

! magenta
*.color5:       #86458b
*.color13:      #a0446c

! cyan
*.color6:       #8eb173
*.color14:      #007d62

! white
*.color7:       #d3cab3
*.color15:      #f4f3e2
