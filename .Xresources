URxvt*geometry:					90x30
URxvt*loginShell:				true
URxvt*scrollBar:				false
URxvt*scrollWithBuffer:				true
!URxvt*fading:					30
URxvt*urgentOnBell:				true
!URxvt*cursorUnderline:				true

*internalBorder:				10

URxvt*perl-ext-common:				default,matcher
URxvt*matcher.button:				1
URxvt*url-launcher:				/usr/bin/chromium

URxvt.font:					xft:Pointfree:size=10
URxvt.boldFont:					xft:Pointfree:bold:size=10

URxvt*depth: 32

!-----------------------------------------                                                                                     
! XFT                                                                                                                          
!-----------------------------------------                                                                                     
Xft.dpi: 96                                                                                                                    
Xft.antialias: 1                                                                                                               
Xft.hinting: 1                                                                                                                 
Xft.hintstyle: hintfull                                                                                                        
Xft.rgba: rgb                                                                                                                  
                   

!BD and FD
*background:                    #ffffff

! red
*color1:        #AC008F
*color9:        #AC008F

!Green
*color2:                        #115322
*color10:                       #115322

!Yellow
*color3:                        #FF002E
*color11:                       #FF002E

! blue
*color4:        #59306B
*color12:       #59306B

! magenta
*color5:        #720033
*color13:       #720033

! cyan
*color6:        #4F5C5B
*color14:       #4F5C5B

! white
*color7:        #2E2D00
*color15:       #2E2D00
