URxvt*geometry:					90x30
URxvt*loginShell:				true
URxvt*scrollBar:				false
URxvt*scrollWithBuffer:				true
!URxvt*fading:					30
URxvt*urgentOnBell:				true
!URxvt*cursorUnderline:				true

*internalBorder:				10

URxvt*perl-ext-common:				default,matcher
URxvt*matcher.button:				1
URxvt*url-launcher:				/usr/bin/firefox

URxvt.font:					xft:Monaco For Powerline:size=8
URxvt.boldFont:					xft:Monaco For Powerline:bold:size=8

URxvt*depth: 32

!-----------------------------------------                                                                                     
! XFT                                                                                                                          
!-----------------------------------------                                                                                     
Xft.dpi: 96                                                                                                                    
Xft.antialias: 1                                                                                                               
Xft.hinting: 1                                                                                                                 
Xft.hintstyle: hintfull                                                                                                        
Xft.rgba: rgb                                                                                                                  
                   

URxvt*background: #e6e6e6
URxvt*foreground: #000000
URxvt*color0:  #FFFFFF
URxvt*color1:  #121212
URxvt*color2:  #A5A5A5
URxvt*color3:  #777777
URxvt*color4:  #999999
URxvt*color5:  #4C4C4C
URxvt*color6:  #2B2B2B
URxvt*color7:  #000000
URxvt*color8:  #9A9A9A
URxvt*color9:  #1C1C1C
URxvt*color10: #A4A4A4
URxvt*color11: #646464
