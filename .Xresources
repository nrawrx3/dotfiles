URxvt*geometry:					90x30
URxvt*loginShell:				true
URxvt*scrollBar:				false
URxvt*scrollWithBuffer:				true
!URxvt*fading:					30
URxvt*urgentOnBell:				true
!URxvt*cursorUnderline:				true

*internalBorder:				10

URxvt*perl-ext-common:				default,matcher
URxvt*matcher.button:				1
URxvt*url-launcher:				/usr/bin/firefox

URxvt.font:					xft:Monaco For Powerline:size=8
URxvt.boldFont:					xft:Monaco For Powerline:bold:size=8

URxvt*depth: 32

!-----------------------------------------
! XFT
!-----------------------------------------
Xft.dpi: 96
Xft.antialias: 1
Xft.hinting: 1
Xft.hintstyle: hintfull
Xft.rgba: rgb


*foreground: #ECE8CF
*background: #2D2D2D
!black
*color0:     #393939
*color8:     #474747
!!red
*color1:     #EE9379
*color9:     #FF8D80
!!green
*color2:     #E2FFC7
*color10:    #DBFFB3
!!yellow
*color3:     #FFFFB9
*color11:    #FFBE64
!!blue
*color4:     #EDE7B4
*color12:    #E15848
!!magenta
*color5:     #E2BEB5
*color13:    #94C7B5
!!cyan
*color6:     #6EB49D
*color14:    #A2C3CC
!!white
*color7:     #dddddd
*color15:    #dddddd

